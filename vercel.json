{
  "rewrites": [
    { "source": "/dashboard", "destination": "/public/dashboard.html" },
    { "source": "/admin", "destination": "/public/admin.html" },
    { "source": "/app", "destination": "/public/app.html" },
    { "source": "/((?!api/).*)", "destination": "/public/$1" }
  ],
  "functions": {
    "api/stripe/webhook.js": {
      "maxDuration": 30
    }
  }
}
